<!--Now you are learning how to create regression suite here-->
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<!-- this doctype is telling system that this file will be suite (smoke, regression)-->

<suite name = "Regression Suite">

    <test name = "Blaze Tests">
        <classes>

            <class name="COM.test.blaze.tests.MacBookTest"></class>

        </classes>
    </test>

    <test name="BankTests">
        <classes>

            <class name="COM.test.bank.tests.BankLoginTest"></class>
            <class name="COM.test.bank.tests.BankManagerTest"></class>
            <class name="COM.test.bank.tests.CustomerPageTest"></class>

        </classes>
    </test>

</suite>




